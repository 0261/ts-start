{
    "extends": [
        "tslint-config-standard"
    ],
    "rules": {
        "ter-indent": [
            true,
            4
        ],
        "semicolon": true,
        "trailing-comma": true,
        "space-before-function-paren": false,
        "eofline": false,
        "unified-signatures": false,
        "no-return-await": false,
        "no-empty": false
    },
    "rulesDirectory": []
}